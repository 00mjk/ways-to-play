{
  "name": "ways-to-play",
  "version": "0.1.0",
  "homepage": "https://games.porg.es/",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.4.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/uuid": "^3.4.5",
    "bootstrap": "^4.3.1",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "responsive-loader": "^1.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "postbuild": "react-snap && purgecss --css build/static/css/*.css --content build/**/*.html --out build/static/css/",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "reactSnap": {
    "puppeteerExecutablePath": "google-chrome-unstable",
    "puppeteerArgs": ["--no-sandbox"],
    "removeScriptTags": true
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "purgecss": "^1.3.0",
    "react-snap": "^1.23.0",
    "sharp": "^0.23.0",
    "source-map-explorer": "^2.0.1"
  }
}
